    "test:performance": "vitest run tests/*performance*.test.ts",
    "test:permit-printing": "vitest run tests/permit-printing-workflow.test.ts tests/permit-printing-e2e.test.ts tests/permit-printing-integration.test.ts",
    "test:production": "NODE_ENV=production vitest run tests/production-environment.test.ts",
    "test:rejected-comments": "vitest run tests/rejected-comments-printing.test.ts tests/rejected-comments-e2e.test.ts tests/rejected-comments-performance.test.ts",
    "test:ui": "vitest --ui",
    "test:workflow": "vitest run tests/workflow-management.test.ts tests/workflow-document-integration.test.ts tests/workflow-document-e2e.test.ts tests/workflow-performance.test.ts"
