  "private": true,
  "description": "Upper Manyame Sub Catchment Council Permit Management System",
  "scripts": {
    "build": "next build",
    "deploy:test": "bash scripts/deployment-test.sh",
    "deploy:update": "bash deployment-update.sh",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest",
    "test:all": "npm run test:notifications && npm run test:workflow && npm run test:documents && npm run test:permit-printing && npm run test:rejected-comments && npm run test:deployment",
    "test:coverage": "vitest --coverage",
    "test:deployment": "vitest run tests/deployment-readiness.test.ts tests/production-environment.test.ts",
